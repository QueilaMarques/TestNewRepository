algoritmo "conteatequantovocequiser"
// Função :
// Autor :     Queila
// Data : 12/05/2023
// Seção de Declarações 
var
      contador, n:inteiro
inicio
      contador <- 0
      Escreva("Ola, vou contar para voce. Até quanto você deseja que eu conte?")
      Leia (n)
            Enquanto (contador <= n) faca
      EscrevaL(contador)
      contador <- contador + 1
      Fimenquanto
fimalgoritmo
