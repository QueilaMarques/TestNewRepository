algoritmo "conteate5"
// Função :
// Autor :
// Data : 26/08/2023
// Seção de Declarações 
var
 cont: inteiro
 S: inteiro
inicio
// Seção de Comandos 
   EscrevaL("Vamos contar de quanto até zero? ")
   Leia (S)
   cont <- (S)
   
   Enquanto cont >= 0 faca
            Escreval (" * ", cont)
            cont <- cont - 1
   FimEnquanto
   Escreval ("Acabei de contar")

fimalgoritmo
